  },
  "type": "module",
  "engines": {
    "homebridge": ">=1.6.0",
    "node": ">=18"
  },
  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "homebridge-unifi-protect",
    "homekit",
    "homekit secure video",
    "hksv",
    "camera",
    "doorbell",
    "ubiquiti",
