  "scripts": {
    "build": "rimraf ./dist && tsc",
    "clean": "rimraf ./dist",
    "lint": "eslint src/**.ts src/devices/**.ts src/ffmpeg/**.ts",
    "jlint": "eslint homebridge-ui/public/**.mjs",
    "postpublish": "npm run clean",
    "prepublishOnly": "npm run lint && npm run build",
